{
    "name": "ko/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Deac Karns",
            "email": "peledies@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "KO\\Fullcalendar\\": "src"
        }
    },
    "scripts": {
        "make:docs": [
            "phpdocs -d ./src -t documentation --title 'KO:FullCalendar'"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "KO\\FullCalendar\\Providers\\FullcalendarServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
